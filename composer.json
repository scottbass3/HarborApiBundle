{
    "name": "scottbass3/harbor-api-bundle",
    "description": "A Wrapper for Harbor API v2 Client library for PHP",
    "type": "symfony-bundle",
    "keywords": ["harbor", "api"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Scottbass3\\HarborApiBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "scottbass",
            "email": "scottbass41@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "php": "^8.3",
        "scottbass3/harbor-api-client": "^1.0",
        "symfony/framework-bundle": "^7.3"
    }
}
